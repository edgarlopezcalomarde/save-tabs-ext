{
    "manifest_version": 3,
    "name": "SaveTabs",
    "description": "Te permite guardar todas las URLs de las ventanas que tengas actualemnte abiertas en tu navegador",
    "version": "1.0",
    "icons": {
        "128": "SaveTabs.png"
    },
    "action": {
        "default_title": "SaveTabs",
        "default_popup": "popUp.html"
    },
    "permissions": [
        "tabs",
        "clipboardRead",
        "clipboardWrite"
    ]
}